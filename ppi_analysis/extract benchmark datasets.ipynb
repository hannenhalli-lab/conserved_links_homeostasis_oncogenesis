    "library(readxl)\n",
    "benchmarks <- data.frame(read_excel(\"benchmark-datasets.xlsx\"))\n",
    "benchmark_list <- list()\n",
    "for(i in 1:length(benchmarks)) {\n",
    "    list21<- benchmarks[2:nrow(benchmarks),i]\n",
    "    print(benchmarks[1,i])\n",
    "    benchmark_list[[benchmarks[1,i]]] <- list21[!is.na(list21)]\n",
    "}"
